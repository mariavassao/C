{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Executar C (Arquivo Atual)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
